{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "YarpApiGatewayServiceDiscovery"
    },
    "eureka": {
      "client": {
        "serviceUrl": "http://eureka-server:8761/eureka/",
        "shouldFetchRegistry": "false",
        "shouldRegisterWithEureka": false,
        "validateCertificates": false
      },
      "instance": {
        "NonSecurePort": 80,
        "HostName": "YarpApiGatewayServiceDiscovery",
        "InstanceId": "YARPSERVICEDISCO"
      }
    }
  }
}

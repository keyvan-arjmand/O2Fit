    <script>
        // Get the button, modal, and iframe
        const openModalButton = document.getElementById('open-modal');
        const openModalButton2 = document.getElementById('open-modal2');
        const modal = document.getElementById('myModal');
        const closeModal = document.getElementsByClassName('close')[0];
        const iframe = document.getElementById('myIframe');
    
        // Add click event listener to open the modal
        openModalButton.addEventListener('click', function() {
            modal.style.display = 'block'; // Show the modal
            // Set the URL for the iframe
            iframe.src = 'https://www.aparat.com/video/video/embed/videohash/YOzMq/vt/frame?autoplay=true';
        });  
        openModalButton2.addEventListener('click', function() {
                       modal.style.display = 'block'; // Show the modal
                       // Set the URL for the iframe
                       iframe.src = 'https://www.aparat.com/video/video/embed/videohash/YOzMq/vt/frame?autoplay=true';
                   });
    
        // Add click event listener to close the modal when close button is clicked
        closeModal.addEventListener('click', function() {
            modal.style.display = 'none'; // Hide the modal
            // Pause the iframe content when modal is closed
            iframe.src = '';
        });
    
        // Close the modal when clicking outside of it
        window.addEventListener('click', function(event) {
            if (event.target == modal) {
                modal.style.display = 'none'; // Hide the modal
                // Pause the iframe content when modal is closed
                iframe.src = '';
            }
        });
    </script>


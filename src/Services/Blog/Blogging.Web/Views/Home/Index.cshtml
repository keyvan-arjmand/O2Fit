    <script>
                    function sendMessage() {
                        var Email = window.$("#Mess_email").val();
                        var Subject = window.$("#Mess_subject").val();
                        var Message = window.$("#Mess_text").val();
                        if (Email && Subject && Message) {
                            window.$.ajax({
                                type: "Post",
                                url: '/Home/SendMessage',
                                data: { Email, Subject, Message },
                                dataType: "json",
                                success: function () {
                                    window.$('.re-cap').prop('disabled', false);
                                    window.$("#Mess_email").val("");
                                    window.$("#Mess_subject").val("");
                                    window.$("#Mess_text").val("");
                                    alert("پیام شما با موفقیت ارسال شد");
                                },
                                beforeSend: function () {
                                    window.$('.re-cap').prop('disabled', true);
                                },
                                error: function (req, status, error) {
                                    window.$('.re-cap').prop('disabled', false);
                                    alert(error);
                                }
                            });
                        } else {
                            alert("اطلاعات خود را تکمیل کنید");
                        }
                    };

    <PackageReference Include="OpenIddict.Client.AspNetCore" Version="4.5.0" />
    <PackageReference Include="OpenIddict.Client.SystemIntegration" Version="4.5.0" />
    <PackageReference Include="OpenIddict.Client.SystemNetHttp" Version="4.5.0" />
    <PackageReference Include="OpenIddict.EntityFrameworkCore" Version="4.5.0" />
    <PackageReference Include="OpenIddict.Quartz" Version="4.5.0" />
    <PackageReference Include="OpenIddict.Validation.AspNetCore" Version="4.5.0" />
    <PackageReference Include="Quartz.Extensions.Hosting" Version="3.6.3" />
  </ItemGroup>

</Project>
